std::string add_binary(uint64_t a, uint64_t b) {
 std::vector<int> container;
   uint64_t added = a+b;
  do { 
     int temp = added %2;
     container.push_back(temp);
   
  } while ((added /= 2) > 0);
   std::reverse(container.begin(), container.end());
  std::string result;
  for (int i = 0; i < container.size(); ++i) {
   result += std::to_string(container[i]);
  }
 return result;
}
