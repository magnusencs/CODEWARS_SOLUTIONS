
std::vector<std::string> wave(std::string y){
  std::vector<std::string>push;
  for (int t = 0; t < y.length(); t++ ){
    if (std::isalpha (y[t]) ){
         std::string herewego = y;
             herewego[t]= std::toupper(herewego[t]);
    push.push_back(herewego);
    }
  
        } return push;
}
